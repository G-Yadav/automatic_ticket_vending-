<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane focusTraversable="true" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.JourneyDetail">
   <children>
      <TextField fx:id="from" layoutX="80.0" layoutY="107.0" prefHeight="26.0" prefWidth="180.0" promptText="From" />
      <TextField fx:id="to" layoutX="80.0" layoutY="154.0" prefHeight="26.0" prefWidth="180.0" promptText="To" />
      <DatePicker fx:id="datePicker" layoutX="82.0" layoutY="196.0" />
      <ComboBox fx:id="choiceBox" layoutX="82.0" layoutY="244.0" prefHeight="28.0" prefWidth="180.0" />
      <Button fx:id="findTrain" layoutX="144.0" layoutY="295.0" mnemonicParsing="false" onMouseClicked="#findTrainButtonClicked" prefHeight="32.0" prefWidth="120.0" text="Find Train" />
      <Label fx:id="usernameLabel" focusTraversable="false" layoutX="467.0" layoutY="21.0" text="Label" />
      <Button fx:id="logout" layoutX="521.0" layoutY="14.0" mnemonicParsing="false" onMouseClicked="#logoutButtonClicked" prefHeight="32.0" prefWidth="65.0" text="Logout" />
      <Label fx:id="warning" focusTraversable="false" layoutX="82.0" layoutY="80.0" style="-fx-text-fill: #ff0000;" text="Label" />
   </children>
</AnchorPane>
